@using AppTest.Models;
@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Index";
}

<br />
<br />
<div class="row">
    <div class="col-2">
        <h2 class="text-info">UserList</h2>
    </div>
    <div class="col-10 text-right">
        <a asp-action="Create" class="btn btn-info"><i class="fas fa-plus"></i>&nbsp; New User</a>
    </div>
</div>
<br />
<div>
    <table class="table table-striped border">
        <tr class="table-info">
            <th>
                @Html.DisplayNameFor(c => c.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(c => c.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(c => c.UserName)
            </th>
            <th></th>
            <th></th>
           
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.UserName</td>
                <td></td>
                <td>
                    <partial name="_PartialView" model="@item.Id" />
                </td>
            </tr>
        }
    </table>
</div>

@section scripts{

}


